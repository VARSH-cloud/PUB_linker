    {
      "parameters": {
        "jsCode": "let htmlBody = '<h1>PubMed Research Papers Found</h1><ul>';\n\n// Loop through all the clean items from the Final Paper List node\nfor (const item of $input.all()) {\n    const title = item.json.Title;\n    const link = item.json.Link;\n\n    // Create an HTML list item for each paper\n    htmlBody += `<li><a href=\"${link}\">${title}</a></li>`;\n}\n\nhtmlBody += '</ul>';\n\n// Output the final body and subject for the next node\nreturn [{\n    json: {\n        subject: `New Research Papers for Hyaluronic Hydrogel`,\n        body: htmlBody\n    }\n}];"
      },

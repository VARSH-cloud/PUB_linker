    {
      "parameters": {
        "jsCode": "const responseData = $input.item.json;\nconst results = []; \n\n// Safely access the array of IDs from the ESearch output\nconst idList = responseData.esearchresult.idlist;\n\nif (idList && idList.length > 0) {\n    // Ensure idList is treated as an array for the loop\n    const paperArticles = Array.isArray(idList) ? idList : [idList];\n    \n    for (const pmid of paperArticles) {\n        // Manually construct the reliable PubMed link\n        const link = `https://pubmed.ncbi.nlm.nih.gov/${pmid}`;\n        \n        results.push({\n            // Since EFetch failed, we use the ID for the title\n            title: `PubMed Paper ID: ${pmid}`,\n            link: link\n        });\n    }\n} else {\n    // Handle zero results\n    return [{ json: { papers: [] } }];\n}\n\nreturn [{ json: { papers: results } }];"
      },
      "type": "n8n-nodes-base.code",
